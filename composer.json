{
  "name": "visuellverstehen/t3telephone",
  "type": "typo3-cms-extension",
  "description": "TYPO3 LinkHandler for handling telephone links via tel protocol.",
  "license": "MIT",
  "authors": [
    {
       "name": "visuellverstehen",
       "email": "kontakt@visuellverstehen.de",
       "homepage": "https://www.visuellverstehen.de"
    }
  ],
  "require": {
    "typo3/cms-core": "8.7 - 9.5"
  },
  "autoload": {
    "psr-4": {
      "VV\\T3telephone\\": "Classes/"
    }
  },
  "scripts": {
      "extension-release": [
          "rm .gitignore",
          "rm .travis.yml",
          "rm -rf .git",
          "rm -rf typo3",
          "rm -rf vendor"
      ]
  }
}
